<div align="center">
<h1>  <U>Change Password </U> </h2>

<form action="#" method="POST">
	<table>
		<tr>
			<td>
				*User Id
			</td>
			<td>
				<input type="text" placeholder="User Id" id="userId" name="userId" />
			</td>
		</tr>
		<tr>
		<td>
			*Old Password
		</td>
		<td>
			<input type="password" placeholder="Old Password" id="oldPassword" name="oldPassword" />
		</td>
	</tr>
			<tr>
		<td>
			*New Password
		</td>
		<td>
			<input type="password" placeholder="New Password" id="newPassword" name="newPassword" />
		</td>
	</tr>
			<tr>
		<td>
			*Confirm New Password
		</td>
		<td>
			<input type="password" placeholder="Confirm Password" id="confNewPassword" name="confNewPassword" />
		</td>
	</tr>
	
	</table>
	
	<br>
	<input type="submit" Value="Change Password" name="clicked"/>
	<input type="submit" Value="Reset Password"  name="clicked"/>

	</form>
	
	<p style="color:red"> (*): Mandatory Fields </p>
	
	<br>
			<h3 style="color: black ; text-align:center;"><%=message%>  </h3>
<br><br>
	<form action="#" method="POST">
	<!--<a style="color: black ; text-align:center" href="#"><b><u>Logout </u> : <%=whichUser%></b></a> -->
	<input type="submit" Value="Logout <%=whichUser%>" name="logout"/>
</form>
</div >

